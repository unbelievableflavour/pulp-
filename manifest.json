{
  "manifest_version": 3,

  "name": "Pulp+",
  "description": "This extension will extend Pulp with some extra features",
  "version": "0.2.1",
  "content_scripts": [{
    "matches": ["https://play.date/pulp/*"],
    "js": ["/js/scriptInjector.js"]
  }],
  "web_accessible_resources" : [{
    "resources": ["/js/injectedScript.js"],
    "matches": ["<all_urls>"]
  }],
  "permissions": [
    "storage"
  ]
}